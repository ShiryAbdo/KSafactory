<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutDirection="rtl"
    android:background="@drawable/smoke_chimney_industrial_29465"
    tools:context="aljentelhosting.ksaksafactory.activites.RegisterCompletTwo">


    <LinearLayout
        android:layout_gravity="center"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >


        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:titleTextColor="@color/white"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:background="@color/colorPrimaryDark"
            >


            <ImageView
                android:id="@+id/go_back"
                android:layout_gravity="end"
                android:layout_width="@dimen/_25sdp"
                android:layout_height="@dimen/_25sdp"
                android:padding="@dimen/_4sdp"
                android:layout_margin="@dimen/_10sdp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:src="@drawable/go_back"
                android:contentDescription="@string/bake_icon" />

            <aljentelhosting.ksaksafactory.others.MyTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="18sp"
                android:textColor="#fff"
                android:layout_gravity="right"
                android:gravity="center"
                android:text="رجوع"
                android:id="@+id/wasf_titel"
                />

        </android.support.v7.widget.Toolbar>
        <ImageView
            android:layout_marginTop="@dimen/_13sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            android:layout_gravity="center"
            android:layout_width="@dimen/_50sdp"
            android:padding="@dimen/_10sdp"
            android:layout_height="@dimen/_50sdp"
            android:id="@+id/imageView"
            android:background="@drawable/storage_copy"
            />
        <TextView
            android:textColor="#01048e"
            android:layout_gravity="center"
            android:textSize="18sp"
            android:layoutDirection="rtl"
            android:text="تسجيل  الشركات والمؤسسات "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />



        <EditText
            android:background="@drawable/fild_redise_blue"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_8sdp"
            android:layout_gravity="center"
            android:layout_width="@dimen/_240sdp"
            android:padding="@dimen/_5sdp"
            android:layoutDirection="rtl"
            android:layout_height="@dimen/_40sdp"
            android:drawablePadding="@dimen/_15sdp"
            android:inputType="textPersonName"
            android:hint="قطاع الخدمي"
            android:ems="10"
            android:drawableRight="@drawable/ic_person_outline_white_24dp"
            android:id="@+id/part_of_service"

            />

        <EditText
            android:layout_marginBottom="@dimen/_8sdp"
            android:layout_gravity="center"
            android:layout_width="@dimen/_240sdp"
            android:layout_height="@dimen/_40sdp"
             android:paddingTop="@dimen/_8sdp"
            android:padding="@dimen/_5sdp"
            android:layoutDirection="rtl"
            android:background="@drawable/fild_redise_blue"
            android:drawableRight="@drawable/ic_mail_outline_black_24dp"
            android:hint="شركة تضامنية"
            android:drawablePadding="@dimen/_15sdp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/sherka_tadamonia" />

        <EditText

            android:hint="كبير جدا 300 عامل أوأكثر"
            android:layout_marginBottom="@dimen/_8sdp"
            android:layout_gravity="center"
            android:layout_width="@dimen/_240sdp"
            android:layout_height="@dimen/_40sdp"
            android:padding="@dimen/_5sdp"
            android:layoutDirection="rtl"
            android:paddingRight="@dimen/_15sdp"
            android:drawablePadding="@dimen/_15sdp"
            android:background="@drawable/fild_redise_blue"
            android:inputType="textPersonName"
            android:drawableRight="@drawable/ic_phone_android_black_24dp"
             android:ems="10"
            android:id="@+id/number_of_worker" />

        <EditText
            android:layout_marginBottom="@dimen/_8sdp"
            android:layout_gravity="center"
            android:layout_width="@dimen/_240sdp"
            android:layout_height="@dimen/_35sdp"
            android:paddingBottom="@dimen/_8sdp"
            android:padding="@dimen/_10sdp"
            android:layoutDirection="rtl"
            android:paddingTop="@dimen/_8sdp"
            android:drawablePadding="@dimen/_15sdp"
            android:drawableRight="@drawable/ic_lock_outline_white_24dp"
            android:hint="عنوان المنشأه"
            android:background="@drawable/fild_redise_blue"
           android:id="@+id/adress_of_company" />

        <EditText
             android:layout_marginBottom="@dimen/_8sdp"
            android:layout_gravity="center"
            android:layout_width="@dimen/_240sdp"
            android:padding="@dimen/_10sdp"
            android:layout_height="@dimen/_40sdp"
            android:paddingTop="@dimen/_8sdp"
            android:drawablePadding="@dimen/_15sdp"
            android:id="@+id/box_email"
            android:layoutDirection="rtl"
            android:background="@drawable/fild_redise_blue"
            android:drawableRight="@drawable/ic_lock_outline_white_24dp"
            android:ems="10"
            android:gravity="right"
            android:hint="الصندوق البريدي"
            android:inputType="textPersonName"
             />

        <EditText
            android:layout_marginBottom="@dimen/_8sdp"
            android:layout_gravity="center"
            android:layout_width="@dimen/_240sdp"
            android:paddingStart="@dimen/_5sdp"
            android:layout_height="@dimen/_40sdp"
            android:gravity="right"
            android:hint="@string/postel_code"
            android:paddingTop="@dimen/_10sdp"
            android:background="@drawable/fild_redise_blue"

             android:drawablePadding="@dimen/_15sdp"
            android:drawableRight="@drawable/ic_lock_outline_white_24dp"
             android:paddingBottom="@dimen/_8sdp"
             android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/postel_code" />



        <Button
            android:textColor="#ffffff"
            android:background="@drawable/login_btn"
            android:layout_gravity="center"
            android:text="أكمل التسجيل "
            android:gravity="center"
            android:textSize="20sp"
            android:layout_width="@dimen/_220sdp"
            android:layout_height="@dimen/_35sdp"
            android:id="@+id/register_as_moassa_two" />


    </LinearLayout>
</FrameLayout>

